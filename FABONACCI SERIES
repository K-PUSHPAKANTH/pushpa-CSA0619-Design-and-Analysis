#include<stdio.h>
 int fibnocci(int num)
{
	if (num==0)
	{
		return 0;
	}
	else if(num==1)
	{
		return 1;
	}
	else
	{
		return fibnocci(num-1)+fibnocci(num-2);
	}
}
 int main()
 {
 	int num;
 	printf("Entr the numbers to be in series: ");
 	scanf("%d",&num);
 	
 	int i;
 	for (i=0;i < num;i++)
 	{
 		printf(" %d ",fibnocci(i));
	 }
	 return 0;
